{
    "name": "orpo/yii2-fingerprintjs",
    "type": "yii2-extension",
    "require": {
        "npm-asset/fingerprintjs--fingerprintjs": "3.4.1"
    },
    "autoload": {
        "psr-4": {
            "Opro\\Yii2Fingerprintjs\\": "src"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
